<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".InventoryData">

    <TextView
        android:id="@+id/totalProduction3"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:height="40dp"
        android:background="#9C27B0"
        android:capitalize="none"
        android:gravity="center"
        android:text="Total Machines Availaible -37"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.853" />

    <TextView
        android:id="@+id/accepted3"
        android:layout_width="260dp"
        android:layout_height="48dp"
        android:height="40dp"
        android:background="#FFC107"
        android:capitalize="none"
        android:gravity="center"
        android:shadowColor="#FFFFFF"
        android:text="Order Planned-37"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.761" />

    <TextView
        android:id="@+id/count3"
        android:layout_width="255dp"
        android:layout_height="41dp"
        android:height="40dp"
        android:background="#E91E63"
        android:capitalize="none"
        android:gravity="center"
        android:shadowColor="#FFFFFF"
        android:text="Total Orders -71"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.519"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.337" />

    <TextView
        android:id="@+id/rejected3"
        android:layout_width="260dp"
        android:layout_height="45dp"
        android:height="40dp"
        android:background="#009688"
        android:capitalize="none"
        android:gravity="center"
        android:text="Order Sheduled-2"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.673" />

    <TextView
        android:id="@+id/tv_Height3"
        android:layout_width="260dp"
        android:layout_height="46dp"
        android:height="40dp"
        android:background="#8BC34A"
        android:capitalize="none"
        android:gravity="center"
        android:text="Raw Material-50 box"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.503" />

    <TextView
        android:id="@+id/height3"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:height="40dp"
        android:background="#2196F3"
        android:capitalize="none"
        android:gravity="center"
        android:text="Completed Orders -9"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.589" />

    <TextView
        android:id="@+id/tv_Count3"
        android:layout_width="260dp"
        android:layout_height="45dp"
        android:height="40dp"
        android:background="#FF5722"
        android:capitalize="none"
        android:gravity="center"
        android:text="Orders In Process-9"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.415" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.141"
        app:srcCompat="@drawable/inventorydata" />

    <TextView
        android:id="@+id/textView1ml"
        android:layout_width="248dp"
        android:layout_height="50dp"
        android:background="#87C38F"
        android:gravity="center"
        android:hapticFeedbackEnabled="false"
        android:shadowColor="#000000"
        android:text="INVENTORY DATA"
        android:textColor="#000000"
        android:textColorHighlight="#FFFFFF"
        android:textColorLink="#6C2626"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <Button
        android:id="@+id/button_generateQR"
        android:layout_width="144dp"
        android:layout_height="52dp"
        android:background="#87C38F"
        android:text="Generate QR Code"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.955" />

</androidx.constraintlayout.widget.ConstraintLayout>